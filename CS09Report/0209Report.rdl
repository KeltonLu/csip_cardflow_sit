  <DataSets>
    <DataSet Name="CS09">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select CancelOASAFile,CancelOASADate,
case @strSource  when '0' then 'OU檔註銷' when '1' then '人工注銷' else '退件注銷' end as CancelOASASource,
CardNo,BlockLog,MemoLog,
case SFFlg when '0' then '未注銷' when '1' then '注銷成功' when '2' then '注銷失敗' when '3' then '人工注銷成功' end as SFFlgName,convert(varchar(10),getdate(),111) as PrintDate
from dbo.tbl_CancelOASA_Detail
where CancelOASAFile=@strFile and CancelOASADate=@strDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@strSource">
            <Value>=Parameters!strSource.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@strFile">
            <Value>=Parameters!strFile.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@strDate">
            <Value>=Parameters!strDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>CS09</DataSourceName>
      </Query>
