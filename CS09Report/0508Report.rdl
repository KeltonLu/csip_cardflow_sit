  <rd:ReportID>4a69ab76-e5c4-4888-b2e5-e417927b967e</rd:ReportID>
  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="CS09">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT B.ID
      ,B.CardNo
      ,S.Imp_Date
      ,S.Maildate
      ,S.Mailno
      ,case S.Info1 when '316' then '投遞不成功' when '223' then '投遞成功' when '240' then '招領中' when '247' then '註銷投遞成功記錄' when '258' then '退件' end as Info1Name
  FROM tbl_Post_Send S left join tbl_Card_BaseInfo B ON S.Maildate=B.Maildate AND 
S.Mailno=B.Mailno
WHERE (@ID ='NULL' OR B.ID=@ID)
AND   (@Imp_DateStart ='NULL' OR S.Imp_Date&gt;=@Imp_DateStart)
AND   (@Imp_DateEnd ='NULL' OR S.Imp_Date&lt;=@Imp_DateEnd)
AND   (@MaildateStart ='NULL' OR S.Maildate&gt;=@MaildateStart)
AND   (@MaildateEnd ='NULL' OR S.Maildate&lt;=@MaildateEnd)
AND   (@Mailno ='NULL' OR S.Mailno=@Mailno)
AND   (@Non_Send_Code ='NULL' OR S.Info1=@Non_Send_Code)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Imp_DateStart">
            <Value>=Parameters!Imp_DateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Imp_DateEnd">
            <Value>=Parameters!Imp_DateEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MaildateStart">
            <Value>=Parameters!MaildateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MaildateEnd">
            <Value>=Parameters!MaildateEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mailno">
            <Value>=Parameters!Mailno.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Non_Send_Code">
            <Value>=Parameters!Non_Send_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>CS09</DataSourceName>
      </Query>
      <Fields>
        <Field Name="ID">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ID</DataField>
        </Field>
        <Field Name="CardNo">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>CardNo</DataField>
        </Field>
        <Field Name="Imp_Date">
