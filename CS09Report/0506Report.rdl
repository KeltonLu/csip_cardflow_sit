  <DataSets>
    <DataSet Name="CS09">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT CardNo
      ,BlockCode
      ,Memo
      ,MemoLog
      ,SFFlg
  FROM tbl_CancelOASA_Detail
WHERE (@ProcessDateStart ='NULL' OR CancelOASADate &gt;=@ProcessDateStart)
AND   (@ProcessDateEnd ='NULL' OR CancelOASADate&lt;=@ProcessDateEnd)
AND   (@Stauts ='NULL' OR SFFlg=@Stauts)
AND   (BlockCode=@BlockCode)
AND   (Memo=@Memo)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProcessDateStart">
            <Value>=Parameters!ProcessDateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProcessDateEnd">
            <Value>=Parameters!ProcessDateEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Stauts">
